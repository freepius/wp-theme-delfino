{
    "name": "freepius/delfino",
    "type": "wordpress-theme",
    "description": "WordPress theme for the photographer Claire Delfino website",
    "keywords": [
        "WordPress",
        "Themes"
    ],
    "homepage": "https://github.com/freepius/wp-theme-delfino#README",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "freepius",
            "homepage": "https://freepius.net"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.0",
        "wptrt/wpthemereview": "^0.2.1",
        "php-parallel-lint/php-parallel-lint": "^1.2.0",
        "wp-cli/i18n-command": "^2.2.5"
    },
    "scripts": {
        "lint:wpcs": "@php ./vendor/squizlabs/php_codesniffer/bin/phpcs",
        "lint:php": "@php ./vendor/bin/parallel-lint --exclude .git --exclude vendor .",
        "make-pot": "wp i18n make-pot . languages/delfino.pot"
    },
    "support": {
        "issues": "https://github.com/freepius/wp-theme-delfino/issues",
        "source": "https://github.com/freepius/wp-theme-delfino"
    }
}
